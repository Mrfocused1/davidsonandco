# GLM API Key (from https://www.z.ai)
GLM_API_KEY=your_glm_api_key_here

# GitHub Personal Access Token (with repo permissions)
# Create at: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# Optional: Vercel Deploy Hook URL for manual deployments
# Create at: Vercel Dashboard > Project > Settings > Git > Deploy Hooks
VERCEL_DEPLOY_HOOK=https://api.vercel.com/v1/integrations/deploy/your_hook_here

# Optional: ScreenshotOne API Key for visual verification
# Create at: https://screenshotone.com (Free tier: 100 screenshots/month)
# Used by the agent to take screenshots and verify visual changes
SCREENSHOT_API_KEY=your_screenshotone_access_key_here
